{
  "name": "mhm/two-factor-authentication-laravel",
  "description": "A package to handle 2FA authentication",
  "type": "library",
  "require": {
    "php": "^8.0",
    "pragmarx/google2fa-qrcode": "^3.0",
    "pragmarx/google2fa-laravel": "^2.0",
    "laravel/framework": "^9.0.0"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "maurohmartinez",
      "email": "maurohmartinez@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "MHM\\TwoFactorAuth": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "MHM\\TwoFactorAuth\\Providers\\TwoFactorAuthServiceProvider"
      ],
      "aliases": {
        "Debugbar": "MHM\\TwoFactorAuth\\Facade"
      }
    }
  }
}
